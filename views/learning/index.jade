html
  head
    include partials/head
  body
    div(id="wrapper")
      include partials/nav
      #page-wrapper
        .container-fluid
          .row
            .col-lg-12
              include partials/header
          .row
            .col-lg-12
              include partials/notifications
          .row
            include partials/panels
          .row
            .col-lg-12
              .panel.panel-default
                .panel-heading
                  h3.panel-title
                    i.fa.fa-bar-chart-o.fa-fw
                    |  Area Chart
                .panel-body
                  h1 test
          .row
            .col-lg-4
            .panel.panel-default
              .panel-heading
                h3.panel-title
                  i.fa.fa-money.fa-fw
                  |  Transactions Panel
              .panel-body
                .table-responsive
                  table.table.table-bordered.table-hover.table-striped
                    thead
                      tr
                        th Tasks
                    tbody
                      each task, index  in tasks.tasks.active 
                        tr
                          td= task

                    .text-right
                      a(href='#')
                        | View All Transactions 
                        i.fa.fa-arrow-circle-right

            .col-lg-4
              include partials/graph
            .col-lg-4
              include partials/post
            .col-lg-4
              include partials/table

            
